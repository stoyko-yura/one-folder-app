  "license": "ISC",
  "scripts": {
    "lint": "eslint . --ext .js,.cjs,.ts",
    "lint:fix": "eslint . --ext .js,.cjs,.ts --fix",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "node dist/index.js",
