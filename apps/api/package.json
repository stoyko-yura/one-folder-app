    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/module-alias": "^2.0.4",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "@one-folder-app/database": "workspace:*",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
